```elixir
defmodule RPS do
  def score_game({opp, you}) do
    case opp do
      :r ->
        case you do
          :r -> 3
          :p -> 6
          :s -> 0
        end

      :p ->
        case you do
          :r -> 0
          :p -> 3
          :s -> 6
        end

      :s ->
        case you do
          :r -> 6
          :p -> 0
          :s -> 3
        end
    end
  end

  def score_move(move) do
    case move do
      :r -> 1
      :p -> 2
      :s -> 3
    end
  end

  def decide_move({opp, exp}) do
    case opp do
      :r ->
        case exp do
          "X" -> :s
          "Y" -> :r
          "Z" -> :p
        end

      :p ->
        case exp do
          "X" -> :r
          "Y" -> :p
          "Z" -> :s
        end

      :s ->
        case exp do
          "X" -> :p
          "Y" -> :s
          "Z" -> :r
        end
    end
  end
end
```

